const mongoose = require("mongoose");
const Schema = mongoose.Schema;

const movieSchema = new Schema({
  airline: {
    type: String,
    enum: ["American", "Southwest", "United"],
  },
  airport: {
    type: String,
    enum: ["AUS", "DFW", "DEN", "LAX", "SAN"],
  },
  flightNo: {
    type: Number,
    required: true,
  },

  departs: {
    type: Date,
    required: true,
  },
  Date,
});
module.exports = mongoose.model("Movie", movieSchema);
